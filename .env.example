# ==================== 应用配置 ====================
APP_NAME="Laravel API"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# ==================== 数据库配置 ====================
DB_CONNECTION=sqlite
# DB_CONNECTION=mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

# ==================== Redis 配置 ====================
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# ==================== 缓存配置 ====================
CACHE_STORE=redis

# ==================== 队列配置 ====================
# 队列驱动：redis（推荐）或 rabbitmq
QUEUE_CONNECTION=redis

# RabbitMQ 配置（可选，如需使用 RabbitMQ 请取消注释）
# QUEUE_CONNECTION=rabbitmq
# RABBITMQ_HOST=localhost
# RABBITMQ_PORT=5672
# RABBITMQ_USER=guest
# RABBITMQ_PASSWORD=guest
# RABBITMQ_VHOST=/
# RABBITMQ_QUEUE=default

# ==================== Session 配置 ====================
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# ==================== JWT 认证配置 ====================
JWT_SECRET=
JWT_TTL=60
JWT_REFRESH_TTL=20160

# ==================== 日志配置 ====================
LOG_CHANNEL=stack
LOG_LEVEL=debug

# ==================== 邮件配置 ====================
MAIL_MAILER=log
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# ==================== Horizon 配置 ====================
# 如使用 Horizon 监控队列
# HORIZON_PREFIX=horizon:
